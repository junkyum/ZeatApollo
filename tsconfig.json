{
    "include": [
        "./src/**/*.tsx",
        "./src/**/*.ts"
    ],
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "lib": ["DOM","ES2020","ESNext.AsyncIterable"],
        "outDir": "dist",
        "rootDir": "src",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
    },
    
}